{
	"workbench.activityBar.location": "side",
	"workbench.colorTheme": "Default Dark Modern",
	"workbench.startupEditor": "none",
	"files.eol": "\n",
	"files.encoding": "utf8",
	"files.trimFinalNewlines": true,
	"files.trimTrailingWhitespace": true,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"editor.insertSpaces": false,
	"editor.fontVariations": false,
	"editor.trimAutoWhitespace": true,
	"editor.multiCursorModifier": "ctrlCmd",
	"telemetry.enableTelemetry": false,
	"redhat.telemetry.enabled": false,
	"java.autobuild.enabled": false,
	"java.codeGeneration.useBlocks": true,
	"java.codeGeneration.hashCodeEquals.useInstanceof": true,
	"java.compile.nullAnalysis.mode": "automatic",
	"java.configuration.updateBuildConfiguration": "interactive",
	"java.completion.enabled": true,
	"java.completion.guessMethodArguments": "auto",
	"java.inlayHints.parameterNames.enabled": "all",
	"java.codeGeneration.toString.codeStyle": "STRING_FORMAT",
	"java.codeGeneration.toString.template": "${object.className} [${member.name()}: ${member.value}, ${otherMembers}]",
	"java.cleanup.actionsOnSave": [
		"addOverride",
		"addDeprecated",
		"instanceofPatternMatch",
		"invertEquals",
		"lambdaExpression",
		"lambdaExpressionFromAnonymousClass",
		"stringConcatToTextBlock",
		"switchExpression",
		"tryWithResource",
	],
	"java.jdt.ls.java.home": "C:\\Program Files\\Java\\jdk-21",
	"workbench.settings.applyToAllProfiles": ["maven.terminal.useJavaHome"],
	"maven.executable.path": "<YOUR MAVEN PATH>\\bin\\mvn.cmd",
	"maven.pomfile.autoUpdateEffectivePOM": true,
	"maven.executable.preferMavenWrapper": true,
	"maven.terminal.useJavaHome": true,
	"rsp-ui.enableStartServerOnActivation": [
		{
			"id": "redhat.vscode-community-server-connector",
			"name": "Community Server Connector",
			"startOnActivation": true
		}
	],
	"security.workspace.trust.untrustedFiles": "open",
}
